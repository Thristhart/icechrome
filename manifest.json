{
	"name": "IceChrome",
	"version": "0.0.2",
	"manifest_version": 2,
	
	"description": "An entirely native IRC client for chrome built using the new Packaged Apps API",
	"icons": {},
	//"default_locale": "en",
	
	
	"app": {
		"background": {
			"scripts": ["launch.js"],
			"transient": true
		}
	},
	
	"permissions": [
		{"socket": ["tcp-connect:*:*"]},
		"storage"
	]
}